        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GoogleJSON"
            android:label="@string/title_activity_google_json" >
        </activity>
        <activity
            android:name=".Actividad_Intent_1"
            android:label="@string/title_activity_actividad__intent_1"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name=".Actividad_Intent_1_2"
            android:label="@string/title_activity_actividad__intent_1_2"
            android:parentActivityName=".Actividad_Intent_1" >
        </activity>
        <activity
            android:name=".CicloDeVida"
            android:label="@string/title_activity_ciclo_de_vida" >
        </activity>
        <activity
            android:name=".Actividad_Intent_2"
            android:label="@string/title_activity_actividad__intent_2" >
        </activity>
        <activity
            android:name=".Actividad_Intent_2_2"
            android:label="@string/title_activity_actividad__intent_2_2" >
        </activity>
    </application>

</manifest>
